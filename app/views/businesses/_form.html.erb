<%= semantic_form_for @business do |f| %>
  <%= f.inputs :name, :address1, :address2, :zip %>

 <h2> Please choose a bartender from the list below </h2>
    <%= f.semantic_fields_for :users do |f| %>
    <%= f.input :id,   :collection => Hash[@business.users.all.map{|b| [b.id,b.username]}]
    %>
  <%end%>
  <p></p>
  <%= f.action :submit %>
  <%= link_to :back %>
<% end %>